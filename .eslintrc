{
  "extends": "google",
  "rules": {
    "space-infix-ops": "error",
    "arrow-spacing": ["error", {"before": true, "after": true}],
    "operator-linebreak": ["error", "before"]
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "esprima"
}
